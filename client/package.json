{
  "name": "client-react-hotel-app",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "eject": "react-scripts eject",
    "compile:sass": "node-sass src/css/styles.scss src/css/styles.css -w",
    "generate:doc": "sassdoc src/css/**/*.scss",
    "test": "cross-env NODE_ENV=test jest --updateSnapshot --config=jest.config.json",
    "test-coverage": "cross-env NODE_ENV=test jest --coverage --config=jest.config.json",
    "test-ci": "cross-env NODE_ENV=test jest --config=jest.config.json --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.22",
    "@types/node": "^11.12.2",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "axios": "^0.18.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "http-proxy-middleware": "^0.19.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-cli": "^24.5.0",
    "node-sass": "^4.13.0",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^8.1.0",
    "react-modal": "^3.1.10",
    "react-redux": "^5.0.7",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.3.0",
    "react-stripe-checkout": "^2.6.3",
    "react-styleguidist": "^9.0.4",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.2.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.7.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
