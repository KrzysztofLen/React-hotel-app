    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "concurrently": "^3.6.0",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "event-stream": "^4.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "jwt-simple": "^0.5.5",
    "lodash.pick": "^4.4.0",
    "mongoose": "^5.1.6",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nodemon": "^1.17.5",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
