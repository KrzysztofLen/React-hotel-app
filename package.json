{
  "name": "root-react-hotel-app",
  "version": "0.1.0",
  "main": "server.js",
  "scripts": {
    "client": "cd client && npm run start",
    "start": "node server.js",
    "start-api": "nodemon server.js",
    "dev": "concurrently \"npm run start-api\" \"npm run client\"",
    "build": "cd client && npm install && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && npm install && npm run build",
    "server:test": "jest --detectOpenHandles server/tests",
    "client:test": "cd client && npm run test",
    "test": "concurrently \"npm run server:test\" \"npm run client:test\""
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-jest": "^24.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "concurrently": "^3.6.0",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.5",
    "event-stream": "^4.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "json-schema": "^0.2.3",
    "jsonwebtoken": "^8.3.0",
    "jwt-simple": "^0.5.5",
    "lodash.pick": "^4.4.0",
    "mongoose": "^5.1.6",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nodemon": "^1.17.5",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "stripe": "^6.1.1",
    "supertest": "^3.4.2"
  }
}
